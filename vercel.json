{
	"functions": {
        "api/rename-list.js": {
            "includeFiles": "secret/**"
        }
    },
    "routes": [
        {
            "src": "/",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            },
            "dest": "/zoom-custom-features.js"
        },
        {
            "src": "/icons",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            },
            "dest": "/public/custom-icons.css"
        },
        {
            "src": "/porto-seguro",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            },
            "dest": "https://raw.githubusercontent.com/edunsouza/zoom-custom-features/porto-seguro/zoom-custom-features.js"
        },
        {
            "src": "/piaui",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            },
            "dest": "https://raw.githubusercontent.com/edunsouza/zoom-custom-features/piaui/zoom-custom-features.js"
        },
        {
            "src": "/es",
            "headers": {
                "Access-Control-Allow-Origin": "*"
            },
            "dest": "https://raw.githubusercontent.com/edunsouza/zoom-custom-features/es/zoom-custom-features.js"
        }
    ]
}